class Solution {
    public int lengthOfLastWord(String s) {
        
        String ans="";
        s=s.trim();

        for(int i=s.length()-1;i>=0;i--){
            char c=s.charAt(i);

            if(c!=' '){
                ans=c+ans;
            }
            else{
                break;
            }
                
            }
            return ans.length();
        }
    }
