class Solution {
    public int findKthLargest(int[] nums, int k) {

        PriorityQueue<Integer> pq=new PriorityQueue(Collections.reverseOrder());

        for(int i=0; i<nums.length;i++){
            pq.offer(nums[i]);
        }

        int x=1;
        int result=0;
        while(x<=k){
            result=pq.poll();
            x++;
        }
        return result;
        
    }
}